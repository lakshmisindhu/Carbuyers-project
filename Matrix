using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp1
{
    class Program
    {
        public void matrix(int rows)
        {
            int i, j;
            for (i = 1; i <= rows; i++)
            {
                for(j=1; j<=i; j++)
                {
                    Console.Write(j);
                }
                Console.WriteLine();
            }
        }
        static void Main(string[] args)
        {
          Program obj = new Program();
           obj.matrix(5);
            
            Console.ReadLine();
        }
    }
}
